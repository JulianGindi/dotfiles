[alias]
      gg = git status
      co = checkout
      ec = config --global -e
      up = !git pull --rebase origin && git prune
      cob = checkout -b
      cm = !git add -A && git commit -m
      save = !git add -A && git commit -m 'SAVEPOINT'
      wip = !git add -u && git commit -m "WIP"
      undo = reset HEAD~1 --mixed
      amend = commit -a --amend
      wipe = !git add -A && git commit -qm 'WIPE SAVEPOINT' && git reset HEAD~1 --hard
      bclean = "!f() { git branch --merged ${1-main} | grep -v " ${1-main}$" | xargs git branch -d; }; f"
      bdone = "!f() { git checkout ${1-main} && git up && git bclean ${1-main}; }; f"
[user]
	name = Julian Gindi
	email = jgindi@riotgames.com
[core]
	editor = /usr/bin/vim
	excludesfile = /Users/julian/dotfiles/git/Python.gitignore
[grep]
  linenumber = true
[color]
	ui = auto
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[commit]
	gpgsign = false
[gpg]
	program = gpg
[init]
	defaultBranch = main
